{
  "extends": "../tsconfig.json",
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": ".",
    "module": "es2015",
    "outDir": "../dist/out-tsc",
    "target": "es5",
    "types": [
      "hammerjs"
    ],
    "noUnusedParameters": false,
    "noUnusedLocals": false,
    "allowUnreachableCode": false,
    "pretty": true
  },
  "exclude": [
    "test.ts",
    "**/*.spec.*"
  ]
}
